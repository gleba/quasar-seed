{
  "name": "SSR/SPA/PWA Template",
  "scripts": {
    "dev": "ts-node ./server",
    "prod": "npm run build && npm run build:server && npm start",
    "build": "vite build && vite build --ssr && npm run build:prerender && ts-node ./scripts/pwa/generate-pwa.ts && npm run build:server",
    "build:prerender": "vite-plugin-ssr prerender",
    "build:server": "vite build -c vite.server.ts",
    "server": "npm run build:server && npm start",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "start": "cross-env NODE_ENV=production node ./dist-server",
    "serve": "serve dist"
  },
  "engines" : {
    "npm" : ">=7.0.0",
    "node" : ">=16.0.0"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.4",
    "fastify-express": "^0.3.3",
    "fastify-static": "^4.5.0",
    "vite-plugin-ssr": "^0.3.60"
  },
  "devDependencies": {
    "https-localhost": "^4.7.1",
    "@swc/core": "^1.2.155",
    "@lmiller1990/v-switch": "^1.1.0",
    "@mdi/font": "^6.5.95",
    "@quasar/vite-plugin": "^1.0.6",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "@vue/compiler-sfc": "^3.2.31",
    "@vuetify/vite-plugin": "^1.0.0-alpha.10",
    "avuef": "^0.12.24",
    "pug": "^3.0.2",
    "quasar": "^2.6.0",
    "sass": "^1.49.9",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "unplugin-icons": "^0.13.4",
    "unplugin-vue-components": "^0.18.0",
    "vite": "^2.8.6",
    "vite-plugin-node": "^0.0.18",
    "vite-plugin-pwa": "^0.11.13",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuetify": "^3.0.0-alpha.10",
    "workbox-core": "^6.5.1",
    "workbox-precaching": "^6.5.1",
    "workbox-routing": "^6.5.1",
    "workbox-window": "^6.5.1"
  }
}
